<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
           xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
           xmlns:context = "http://www.springframework.org/schema/context"
           xsi:schemaLocation = "http://www.springframework.org/schema/beans
                     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                     http://www.springframework.org/schema/context
                     http://www.springframework.org/schema/context/spring-context-3.0.xsd">
                     
                     <bean id="question" class="que2.Question">
                             <property name="questionId"  value="111"/>
                             <property name="question" value="What is Spring?" />
                             <property name="answers">
                             <list>
                                 <value>Spring is a lightweight framework.</value>
                                 <value>Spring is the most popular application development framework for enterprise Java.</value>
                            </list>
                            </property>
                     </bean>
                     <bean id="question1" class="que2.Question1">
                             <property name="questionId"  value="222"/>
                             <property name="question" value="What is Spring?" />
                             <property name="answersSet">
                             <set>
                                 <value>Spring is a lightweight framework.</value>
                                 <value>Spring is the most popular application development framework for enterprise Java.</value>
                            </set>
                            </property>
                     </bean>
                      <bean id="question2" class="que2.Question2">
                             <property name="questionId"  value="333"/>
                             <property name="question" value="What is Spring?" />
                             <property name="answersMap">
                             <map>
                                <entry key="1">
                                 <value>Spring is a lightweight framework.</value>
                                 </entry>
                                 <entry key="2">
                                 <value>Spring is the most popular application development framework for enterprise Java.</value>
                                 </entry>
                            </map>
                            </property>
                     </bean>

                    
</beans>                       
             